<mvc:View controllerName="metallo.PrintQR.controller.QRCode" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<VBox alignItems="Center">
					       <Image class="qrCodfe" id="qrImageId"/>
					       <Button press="onButtonPress" text="Pdf Download" type="Emphasized" class="pdfButton"/>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>